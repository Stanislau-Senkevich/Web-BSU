<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>WelcomeFilter</filter-name>
    <filter-class>by.bsu.lab4.filter.WelcomeFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/application/list</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/car/list</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/trip/list</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/user/list</url-pattern>
  </filter-mapping>

  <filter-mapping>
    <filter-name>WelcomeFilter</filter-name>
    <url-pattern>/css/</url-pattern>
  </filter-mapping>
</web-app>
